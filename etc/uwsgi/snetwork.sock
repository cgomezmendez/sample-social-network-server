[uwsgi]
#application's base folder
base = /home/cgomez/snetwork/

#python module to import
app = snetwork
module = %(app).main

home = %(base)/env
virtualenv = %(base)/env
pythonpath = %(base)

#socket file's location
socket = /home/cgomez/snetwork/etc/uwsgi/snetwork.sock

#permissions for the socket file
chmod-socket    = 666

#the variable that holds a flask application inside the module imported at line #6
callable = app

#location of log files
logto = /home/cgomez/snetwork/log/snetwork.log
